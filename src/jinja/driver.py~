print("start")

#
# argv[1] = csv filename
#
if __name__ == "__main__":
    if len(sys.argv) > 1:
        file_name = sys.argv[1]
        md = MastodonDiagnostic()
        md.execute(file_name)
    else:
        print("need filename")
        exit(1)

print("stop")

